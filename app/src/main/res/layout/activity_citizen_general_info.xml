<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".citizen_general_info">

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@color/colorPrimary"
        android:paddingTop="20dp"
        android:text="GENERAL INFORMATION"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="216dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="Name"
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />


    <TextView
        android:id="@+id/txtname"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/txtage"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtname" />

    <TextView
        android:id="@+id/txtgender"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtage" />

    <TextView
        android:id="@+id/txtmarital_status"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtgender" />

    <TextView
        android:id="@+id/txtcontribute"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtmarital_status" />

    <TextView
        android:id="@+id/txtrelation"
        android:layout_width="180dp"
        android:layout_height="45dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtcontribute" />

    <TextView
        android:id="@+id/txtbeenaway"
        android:layout_width="177dp"
        android:layout_height="53dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="196dp"
        android:background="@mipmap/question_background"
        android:paddingLeft="14dp"
        android:paddingTop="10dp"
        android:text=""
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtrelation"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="216dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="age"
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="216dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="gender"
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="216dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="20dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="marital status"
        android:textAllCaps="true"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="196dp"
        android:layout_height="84dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="23dp"
        android:layout_marginBottom="184dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="Have you been away in the last 12 months?"
        android:textAllCaps="false"
        android:textColor="@color/black_semi_transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.25" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="186dp"
        android:layout_height="57dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="24dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="Contribute to househod income?"
        android:textAllCaps="false"
        android:textColor="@color/black_semi_transparent"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="195dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        android:paddingLeft="8dp"
        android:paddingTop="10dp"
        android:text="Relation to household owner."
        android:textAllCaps="false"
        android:textColor="@color/black_semi_transparent"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

</androidx.constraintlayout.widget.ConstraintLayout>